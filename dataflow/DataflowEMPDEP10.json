{
	"name": "DataflowEMPDEP10",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_EMP10",
						"type": "DatasetReference"
					},
					"name": "Employee10"
				},
				{
					"dataset": {
						"referenceName": "Ds_DEP10",
						"type": "DatasetReference"
					},
					"name": "Dep10"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Output10",
						"type": "DatasetReference"
					},
					"name": "OutputData10"
				}
			],
			"transformations": [
				{
					"name": "joinEMPdataDEPdata"
				},
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmpID as string,",
				"          Name as string,",
				"          Country as string,",
				"          Dep as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Employee10",
				"source(output(",
				"          Dep as string,",
				"          DepName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Dep10",
				"ModifyColumns1, Dep10 join(EmpID == Dep10@Dep,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinEMPdataDEPdata",
				"Employee10 derive(Country = upper(Country)) ~> ModifyColumns1",
				"joinEMPdataDEPdata sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['EMPDEPDATA10'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          EmpID,",
				"          EmpName = Name,",
				"          {Modify Country} = Country,",
				"          DepName",
				"     ),",
				"     partitionBy('hash', 1)) ~> OutputData10"
			]
		}
	}
}